/*===========================按钮样式================================*/

/**biao ge bian se**/
tr.odd{background: #F8F8FF;}
tr.highlight {
	background: #FFF3BF;
}

tr.selected {
	background: #FFF3BF;
}

.btn_add { /*添加*/
	width: 57px;
	height: 22px;
	border: 0px scrollbar;
	azimuth: center;
	background: url("../images/button/btn_add.gif") no-repeat;

}

