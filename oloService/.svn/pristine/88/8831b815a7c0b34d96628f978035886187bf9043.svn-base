package com.sys.wx.model;

import java.io.Serializable;

/**
 * 产品属性模型类
 * @author sk
 *
 */
public class AttributeModel implements Serializable{
	/**
	 * 序列化
	 */
	private static final long serialVersionUID = 7434485566463293990L;

	private Integer a_id;
	//名称
	private String a_name;
	//图片
	private String a_pic;
	//价格
	private Double a_price;
	//单位
	private String a_unit;
	//备注
	private String a_remark;
	//所属分类
	private Integer a_tid;
	
	private String at_name;

	public Integer getA_id() {
		return a_id;
	}

	public void setA_id(Integer a_id) {
		this.a_id = a_id;
	}

	public String getA_name() {
		return a_name;
	}

	public void setA_name(String a_name) {
		this.a_name = a_name;
	}

	public String getA_pic() {
		return a_pic;
	}

	public void setA_pic(String a_pic) {
		this.a_pic = a_pic;
	}

	public Double getA_price() {
		return a_price;
	}

	public void setA_price(Double a_price) {
		this.a_price = a_price;
	}

	public String getA_unit() {
		return a_unit;
	}

	public void setA_unit(String a_unit) {
		this.a_unit = a_unit;
	}

	public String getA_remark() {
		return a_remark;
	}

	public void setA_remark(String a_remark) {
		this.a_remark = a_remark;
	}

	public Integer getA_tid() {
		return a_tid;
	}

	public void setA_tid(Integer a_tid) {
		this.a_tid = a_tid;
	}

	public String getAt_name() {
		return at_name;
	}

	public void setAt_name(String at_name) {
		this.at_name = at_name;
	}
}
