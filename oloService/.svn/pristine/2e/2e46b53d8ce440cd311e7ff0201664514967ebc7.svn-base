package com.sys.wx.model;
/** 
 * 按钮的基类 
 *  所有一级菜单、二级菜单都共有一个相同的属性，那就是name
 * @author mxy  
 */  
public class Button {  
    private String name;   
    public String getName() {  
        return name;  
    }  
   

	public void setName(String name) {  
        this.name = name;  
    }  
}