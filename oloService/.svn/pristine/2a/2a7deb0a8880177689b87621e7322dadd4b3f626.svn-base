
.uito {
	width: 100%;
	height: 23px;
	list-style: none;
	border: 0px;
}

.uito ul {
	width: 100%;
	float: left;
	list-style: none;
}

.uito ul li {
	float: left;
	text-align: center;
	margin-left: 25px;
	list-style: none;
}

body {
	height: 100%;
	width: 100%;
	margin: 0px;
}

.hidden {
	display: none;
}

.h {
	float: left;
}

H2.loading {
	border: 0;
	font-size: 24px;
	font-weight: normal;
	margin: 30% 0 0 40%;
}

#tab_menu {
	padding: 0px 0px 0px 0px; 
	width: 100%;
	height: 25px;
	border: solid 0px #cccccc;
	overflow: hidden;
}

#page {
	width: 100%;
	border: solid 0px #cccccc;
	height: expression(parentNode . parentNode . offsetHeight-25);
}

#top {
	overflow-y: hidden;
	overflow-x: hidden;
	height: 80px;
	margin: 0 auto;
	padding: 0;

}

#exit {
	float: right;
	margin-top: 55px;
	font: bolder;
	overflow-x: hidden;
	overflow: hidden;
	font-weight: normal;
	color: #f2f8ff;
	font-weight: bold;
}

#exit a {
	color: #f2f8ff;
	text-decoration: none
}

#exit a:hover {
	color: #fff;
	text-decoration: underline
}

